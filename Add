#include <iostream>
using namespace std;
 
 inline double add(double x, double y) //Made function inline for faster execution
 {
    return x+y;
 }
// Driver's code
int main()
{
    cout << "Enter first number: \n";
    cin >> num1;
    cout << "Enter second number: \n";
    cin >> num2;
    cout << "Addition of these two numbers is " << add(num1,num2);
    return 0;
}
